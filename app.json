{
  "expo": {
    "name": "mipa",
    "description": "Mobile interview prep app for developers",
    "slug": "mipa",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "packagerOpts": {
      "assetExts": ["md"]
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.algodaily.app",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.774296032789-jdm096si9nakochbl4cd0icu820nqoaa"
        }
      }
    },
    "android": {
      "package": "com.algodaily.app"
    }    
  }
}
